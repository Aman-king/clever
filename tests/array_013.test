Testing Array<>::find()
==CODE==
import std.io as io;

Array<String> a;

a.push('Clever');
a.push('Programming');
a.push('Language');

io::println(a.find('Clever'));
io::println(a.find('Cleverasd'));
io::println(a.find('Language'));

Pair<Int, Bool> p1(1, true), p2(3, false), p3(-1, true), p4(-1, false);

Array<Pair<Int, Bool> > b;
b.push(p1);
b.push(p2);
b.push(p3);

io::println(b.find(p2));
io::println(b.find(p4));

