Testing ReflectionFunction methods
==CODE==
import std;

use println as std.io::println;
use ReflectionFunction as std.reflection::ReflectionFunction;

ReflectionFunction f("std.math::max");
println(f.getReturnType(),
	f.isInternal(),
	f.isUserDefined(),
	f.getNumRequiredArgs(),
	f.isVariadic());


ReflectionFunction z("println");

println(z.isVariadic());
