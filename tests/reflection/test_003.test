Testing ReflectionFunction methods
==CODE==
import std;

use println as std.io::println;
use ReflectionFunction as std.reflection::ReflectionFunction;

ReflectionFunction a("std.math::max");
println(a.getName(), a.getArgs());

ReflectionFunction b("std.sys::getcwd");
println(b.getName(), b.getArgs());

ReflectionFunction c("getcwd2");
println(c.getName(), c.getArgs());
