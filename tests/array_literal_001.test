Testing Array<> literal
==CODE==
import std.io.* as io;
import std.math.* as math;

io::println([10, 12, 14]);

Void output(Array<String> arr)
{
	io::print('Name: ', arr[0], "\n");
	io::print('Version: ', arr[1], "\n");
}

output(['Clever', '0.1-alpha']);

const Double pi = math::acos(-1.0), sqrt2 = math::sqrt(2.0);

io::println([sqrt2, pi, math::cos(0.0)].at(1));

==RESULT==
\[10, 12, 14\]
Name: Clever
Version: 0.1-alpha
3.14159
