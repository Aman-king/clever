Testing Array's methods
==CODE==
import std.io as io;

Array<Int> x;

for (Int i = 0; i < 5; ++i) {
	x.push(i);
}

for (Int j = 4; j >= 0; --j) {
	io::println(x.at(j));
}

io::println(x.size());

if (x.isEmpty()) {
	io::println("fail!");
}
else {
	x.clear();

	if (false == x.isEmpty()) {
		io::println("fail!");
	}
}

io::println("done!");
