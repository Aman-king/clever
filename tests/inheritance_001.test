Inheritance test #1
==CODE==
import std.io;
import std.reflection;

use print as std.io::print;
use get_type as std.reflection::get_type;

Void my_println(Object obj)
{
	print(get_type(obj), ': ', obj, "\n");
}

my_println(10);
my_println('clever');
my_println(-23.04);
my_println(true);
==RESULT==
Int: 10
String: clever
Double: -23.04
Bool: true
