import std.*;
import std.ffi;


var id = 0;

process t[2] {
	var h;
	var mid = 0;
	critical {
		h =  FFILib.new("nffi");
		mid = id;
		++id;
	}

	var cnt = 0;
	while (cnt < 10) {

		critical {
			if (mid == 0) {
				h.call("printExt", "v", "ola");
			} else {
				h.call("printExt", "v", "ola 2");
				println(h.call("add", "i", 2, 3));
			}
		}

		++cnt;
	}
}

t.run();
